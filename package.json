{
  "name": "my-comments",
  "private": true,
  "description": "",
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@babel/preset-env": "7.20.2",
    "babel-loader": "9.1.0",
    "copy-webpack-plugin": "11.0.0",
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "10.4.0",
    "del": "6.0.0",
    "css-loader": "6.7.2",
    "style-loader": "3.3.1",
    "eslint": "8.28.0",
    "eslint-config-htmlacademy": "8.0.0",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.0",
    "webpack-dev-server": "4.11.1"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "lint": "npm run editorconfig && npm run stylelint",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "build": "webpack --mode production",
    "start": "webpack serve --mode development --open --history-api-fallback"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all",
    "not < 0.25%"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.scss"
  ],
  "engines": {
    "node": "18"
  },
  "type": "module"
}
